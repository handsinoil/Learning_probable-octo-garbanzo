   "source": [
    "# fibonacci memory\n",
    "from typing import Dict\n",
    "memo: Dict[int, int] = {0: 0, 1: 1} # basic case\n",
    "def fib_memo(n: int) -> int:\n",
    "    if n not in memo:\n",
    "        memo[n] = fib_memo(n-1) + fib_memo(n-2) # memorization\n",
    "    return memo[n]\n",
    "print(fib_memo(50))"
   ]
